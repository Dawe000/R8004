{
  "name": "@erc8001/agent-task-sdk",
  "version": "0.1.0",
  "description": "TypeScript SDK for ERC8001 Agent Task Escrow - client and agent interactions",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "test": "jest --testPathIgnorePatterns=integration.test --testPathIgnorePatterns=live.test",
    "test:integration": "jest integration.test",
    "test:live": "SDK_LIVE_TESTNET=1 jest live.test",
    "test:live:flow": "SDK_LIVE_TESTNET=1 SDK_LIVE_RUN_FLOW=path-c,path-d jest live.test",
    "test:ipfs:live": "npx tsx script/test-ipfs-live.ts",
    "typecheck": "tsc --noEmit"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "dependencies": {
    "ethers": "^6.9.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "dotenv": "^17.2.4",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.0"
  }
}
