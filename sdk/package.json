{
  "name": "@erc8001/agent-task-sdk",
  "version": "0.1.0",
  "description": "TypeScript SDK for ERC8001 Agent Task Escrow - client and agent interactions",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js --testPathIgnorePatterns=integration.test",
    "test:integration": "node --experimental-vm-modules node_modules/jest/bin/jest.js integration.test",
    "typecheck": "tsc --noEmit"
  },
  "files": ["dist", "README.md"],
  "dependencies": {
    "ethers": "^6.9.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.0"
  }
}
